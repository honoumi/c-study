const char* my_strstr(const char* p1, const char* p2)
{
	const char* s1 = p1;
	const char* s2 = p2;
	const char* cur = p1;
	if (*p2 == '\0')
	{
		return NULL;
	}
	while (*cur)
	{
		s1 = cur;
		s2 = p2;
		while ((*s1 != '/0') && (*s2 != '\0') && (*s1 == *s2))
		{
			s1++;
			s2++;
		}
		if (*s2 == '\0')
		{
			return cur;
		}
		cur++;
	}
	return NULL;
}
