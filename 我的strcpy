void my_strcpy(char* d, char* t)
{
	assert(d != NULL);
	assert(t != NULL);

	int k = strlen(t);
	int i = 0;
	for (i = 0; i <= k; i++)
	{
		*(d + i) = *(t + i);
	}


}
void my_strcpy2(char* d, char* t)char* my_strcpy(char* d,const char* t)
{
	assert(d != NULL);
	assert(t != NULL);
	char* ret = d;
	while (*d++ = *t++)
	{
		;
	}
	//返回目的代码的起始地址！
	return ret;

}



int main()
{
	char arr[] = "asdfg";
	char arr2[] = "qwe";
	my_strcpy(arr, arr2);
	printf("%s", arr);

}
